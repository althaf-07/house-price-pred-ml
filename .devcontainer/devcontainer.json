            "extensions": [
                "-ms-python.autopep8",
                "-dbaeumer.vscode-eslint",
                "-ms-python.python",
                "-ms-python.vscode-pylance"
            ]
        }
    },
    "postCreateCommand": "./.devcontainer/devcontainer_script.sh"
}
