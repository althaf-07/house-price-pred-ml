{
    "name": "althaf-07/house-price-pred-ml",
    "build": {
        "context": "..",
        "dockerfile": "./Dockerfile"
    },
    "runArgs": [
        "--name",
        "dc_house-price-pred-ml",
        "--env-file",
        "./.devcontainer/devcontainer.env"
    ],
    "customizations": {
